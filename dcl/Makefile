top=../
ede_FILES=Project.ede Makefile

dcl_SOURCES=gettoken.cpp utils.cpp dcl.cpp main.cpp
dcl_OBJ= gettoken.o utils.o dcl.o main.o
CXX= g++
CXX_COMPILE=$(CXX) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)
CXX_DEPENDENCIES=-Wp,-MD,.deps/$(*F).P
CXX_LINK=$(CXX) $(CFLAGS) $(LDFLAGS) -L.
VERSION=1.0
DISTDIR=$(top)Solutions-$(VERSION)/dcl
top_builddir = ..

DEP_FILES=.deps/gettoken.P .deps/utils.P .deps/dcl.P .deps/main.P .deps/dcl.P

all: dcl

DEPS_MAGIC := $(shell mkdir .deps > /dev/null 2>&1 || :)
-include $(DEP_FILES)

%.o: %.cpp
	@echo '$(CXX_COMPILE) -c $<'; \
	$(CXX_COMPILE) $(CXX_DEPENDENCIES) -o $@ -c $<

dcl: $(dcl_OBJ)
	$(CXX_LINK) -o $@ $^ $(LDDEPS)

tags: 


clean:
	rm -f *.mod *.o *.obj .deps/*.P .lo

.PHONY: dist

dist:
	mkdir $(DISTDIR)
	cp $(dcl_SOURCES) $(ede_FILES) $(DISTDIR)

